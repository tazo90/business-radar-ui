{
  "name": "business-radar-ui",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:widgets": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"./src/widgets/tsconfig.json\" webpack --mode=production --config ./src/widgets/webpack.config.widget.js",
    "start": "next start",
    "lint": "next lint",

    "db:deploy": "yarn prisma migrate deploy",
    "db:migrate": "yarn prisma migrate dev --schema=./src/lib/prisma/schema.prisma",
    "db:nuke": "docker-compose down --volumes --remove-orphans",
    "db:seed": "yarn prisma db seed",
    "db:studio": "yarn prisma studio",
    "db:generate": "prisma generate && prisma format --schema=./src/lib/prisma/schema.prisma"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.12.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@turf/turf": "^6.5.0",
    "axios": "^0.26.0",
    "cssnano": "^5.1.7",
    "maplibre-gl": "^2.1.6",
    "next": "12.1.0",
    "next-auth": "^4.3.1",
    "next-images": "^1.8.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.27.1",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.16",
    "react-redux": "^7.2.6",
    "react-scroll": "^1.8.6",
    "react-window": "^1.8.6",
    "sass": "^1.49.9",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "@types/react-scroll": "^1.8.3",
    "autoprefixer": "^10.4.2",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "dotenv": "^16.0.0",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "file-loader": "^6.2.0",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prisma": "^3.12.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.23",
    "ts-loader": "^9.2.6",
    "tsconfig-paths": "^3.12.0",
    "typescript": "4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
