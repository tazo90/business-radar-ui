{
  "strict": true,
  "include": ["**/*.ts", "**/*.tsx"],
  "baseUrl": ".",
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "paths": {
      "@assets/*": [
        "../assets/*"
      ],
      "@components/*": [
        "../components/*"
      ],
      "@containers/*": [
        "../containers/*"
      ],
      "@constants/*": [
        "../constants/*"
      ],
      "@api/*": [
        "../api/*"
      ],
      "@settings/*": [
        "../settings/*"
      ],
      "@slices/*": [
        "../slices/*"
      ],
      "@styles/*": [
        "../styles/*"
      ],
      "@utils/*": [
        "../utils/*"
      ]
    },
  }
}
